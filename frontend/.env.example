# =============================================================================
# Frontend Environment Configuration Example
# =============================================================================
# Copy this file to .env.local and update with your actual values
# This file is specifically for frontend-only configurations

# =============================================================================
# Application Environment
# =============================================================================
NODE_ENV=development
VITE_APP_ENV=development

# =============================================================================
# API Configuration
# =============================================================================
VITE_API_BASE_URL=http://localhost:5000/api/v1
VITE_WS_URL=ws://localhost:5000

# =============================================================================
# Supabase Configuration (Client-side)
# =============================================================================
# Get these from your Supabase project settings > API
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Note: Only use the anonymous key in the frontend
# Never expose service role keys in client-side code

# =============================================================================
# Application Configuration
# =============================================================================
VITE_APP_NAME=LETHCON Cybersecurity Training
VITE_APP_DESCRIPTION=Advanced cybersecurity training platform
VITE_APP_VERSION=1.0.0

# =============================================================================
# Feature Flags
# =============================================================================
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_DEBUG_MODE=true

# =============================================================================
# External Services (Client-side)
# =============================================================================
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
VITE_GOOGLE_ANALYTICS_ID=your-ga-tracking-id

# =============================================================================
# Development Settings
# =============================================================================
VITE_DEV_TOOLS=true
VITE_LOG_LEVEL=debug
