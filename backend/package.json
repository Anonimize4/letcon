    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^7.0.0",
    "supertest": "^7.1.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
